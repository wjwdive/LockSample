source 'https://cdn.cocoapods.org/' # 优先使用CDN源
platform :ios, '13.0'

def core_pods
  pod 'MJRefresh', '= 3.7.9'
  pod 'Masonry', '~> 1.1.0'
  pod 'AFNetworking', '=4.0.1'
  
end



target 'LockSample' do
  # Comment the next line if you don't want to use dynamic frameworks
  # 所有 Pod 都将作为动态 Framework 接入项目。可支持 Swift Pods，避免静态库冲突。
  use_frameworks!

  core_pods

end